{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "paths": {
      "@utils/*": ["./webapp/javascript/util/*"],
      "@models/*": ["./webapp/javascript/models/*"]
    }
  },
  "exclude": ["node_modules"]
}
